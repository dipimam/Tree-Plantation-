
@{
    ViewBag.Title = "ChangeInformation";
    Layout = "~/Views/Shared/dashboard.cshtml";
}
@Styles.Render("~/Content/Css/login.css")
@model Tree_Plantation.Models.admin


<form action="/Admin/ChangeInformation" method="post">


    <div class="form-group">
        <label for="a_name">Name</label>
        <input type="text" class="form-control" id="a_name" name="a_name" value="@Model.a_name" placeholder="Enter Name">

    </div>
    <div class="form-group">
        <label for="a_phone">Phone</label>
        <input type="number" class="form-control" id="a_phone" name="a_phone" value="@Model.a_phone" placeholder="Enter Phone">

        <span class="text-danger"></span>

    </div>

    <div class="form-group">
        <label for="a_address">Address</label>
        <input type="text" class="form-control" id="a_address" name="a_address" value="@Model.a_address" placeholder="Enter Address">


    </div>



    <button type="submit" class="btn btn-primary">Submit</button>
</form>


